{
  "name": "MUVR",
  "productName": "MUVR Native Caster",
  "description": "Multi-User Virtual REality",
  "version": "0.0.1",
  "private": true,
  "author": "remy <remaclematt@gmail.com>",
  "copyright": "Â© 2019 Matthew Remacle",
  "homepage": "https://muvr.xyz",
  "main": "app/background.js",
  "build": {
    "appId": "com.example.electron-boilerplate",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "muvr-dia-install": "npm install ../muvr-dia/muvr-dia-1.0.0.tgz --save",
    "preunit": "webpack --config=build/webpack.unit.config.js --env=test --display=none",
    "unit": "electron-mocha temp/specs.js --renderer --require source-map-support/register",
    "pree2e": "webpack --config=build/webpack.app.config.js --env=test --display=none && webpack --config=build/webpack.e2e.config.js --env=test --display=none",
    "e2e": "mocha temp/e2e.js --require source-map-support/register",
    "test": "npm run unit && npm run e2e",
    "start": "node build/start.js",
    "release": "webpack --config=build/webpack.app.config.js --env=production && electron-builder",
    "release-all": "powershell ./build-all.ps1"
  },
  "dependencies": {
    "electron-store": "^5.2.0",
    "fs-jetpack": "^2.4.0",
    "muvr-dia": "file:../muvr-dia/muvr-dia-1.0.0.tgz",
    "node-abi": "^2.19.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "babel-loader": "^8.2.1",
    "chai": "^4.1.0",
    "css-loader": "^3.6.0",
    "electron": "^15.5.5",
    "electron-builder": "^22.9.1",
    "electron-mocha": "^8.2.2",
    "electron-rebuild": "^1.11.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "mocha": "^6.2.3",
    "node-gyp": "^5.1.1",
    "source-map-support": "^0.5.19",
    "spectron": "^6.0.0",
    "style-loader": "^0.23.1",
    "uglify-js": "^3.12.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^4.1.3",
    "webpack-node-externals": "^1.7.2",
    "webrtc-adapter": "^7.7.0"
  }
}
